{
  "name": "deepword",
  "version": "1.0.0",
  "description": "Web editor based on Monaco",
  "main": "lib/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudcmd/deepword"
  },
  "keywords": [
    "editor",
    "express",
    "middleware"
  ],
  "author": "coderaiser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cloudcmd/deepword/issues"
  },
  "homepage": "https://github.com/cloudcmd/deepword",
  "bin": {
    "deepword": "bin/deepword.js"
  },
  "scripts": {
    "lint": "redrun lint:*",
    "fixlint": "redrun -p lint:* --fix",
    "lint:eslint-bin": "eslint --rule 'no-console:0' bin",
    "lint:eslint-lib": "eslint lib",
    "6to5:lib": "babel lib -d legacy/lib",
    "6to5:bin": "babel bin -d legacy/bin",
    "wisdom": "npm run build",
    "postpublish": "npm i -g",
    "build": "redrun 6to5:* legacy:*",
    "legacy:index": "echo \"module.exports = require('./lib/server.js');\" > legacy/index.js",
    "legacy:package": "echo \"module.exports = require('../package');\" > legacy/package.js"
  },
  "dependencies": {
    "ashify": "^1.0.1",
    "express": "^4.14.0",
    "join-io": "^1.4.0",
    "mollify": "^1.0.4",
    "monaco-editor": "^0.6.1",
    "os-homedir": "^1.0.0",
    "patchfile": "^1.0.1",
    "readjson": "^1.1.0",
    "rendy": "^1.1.0",
    "restafary": "^1.6.0",
    "socket-file": "^1.3.0",
    "socket.io": "^1.4.3",
    "try-catch": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "eslint": "^3.6.1",
    "redrun": "^5.8.4"
  }
}
