{
  "name": "deepword",
  "version": "3.0.6",
  "description": "Web editor based on Monaco",
  "main": "server/index.js",
  "bin": {
    "deepword": "bin/deepword.js"
  },
  "scripts": {
    "start": "bin/deepword.js package.json",
    "start:dev": "NODE_ENV=development npm start",
    "test": "tape '{client,common,server}/**/*.spec.js'",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "watch:test": "redrun watcher:test -- \"redrun test\"",
    "watch:coverage": "redrun watcher:test -- \"redrun coverage\"",
    "watcher:test": "nodemon -e spec.js -w client -w server -w common -x ",
    "lint": "redrun -p lint:*",
    "fix:lint": "redrun -p lint:* -- --fix",
    "lint:bin": "eslint --rule 'no-console:0' --config .eslintrc.server bin",
    "lint:client": "eslint --config .eslintrc.client client",
    "lint:server": "eslint --config .eslintrc.server server common --ignore-pattern *.spec.js",
    "wisdom": "npm run build",
    "build": "redrun clean mkdir build:* cp:*",
    "cp:socket.io:dist": "cp node_modules/socket.io-client/dist/socket.io.js dist/socket.io.js",
    "cp:socket.io:dist-dev": "cp node_modules/socket.io-client/dist/socket.io.js dist-dev/socket.io.js",
    "cp:socket.io.map:dist": "cp node_modules/socket.io-client/dist/socket.io.js.map dist/socket.io.js.map",
    "cp:socket.io.map:dist-dev": "cp node_modules/socket.io-client/dist/socket.io.js.map dist-dev/socket.io.js.map",
    "build-progress": "webpack --progress",
    "build:client": "npm run build-progress -- --mode production",
    "build:client:dev": "NODE_ENV=development npm run build-progress -- --mode development",
    "watch:client": "nodemon -w client -x \"npm run build:client\"",
    "watch:client:dev": "NODE_ENV=development npm run watch:client",
    "watch:lint:bin": "redrun watcher:js -- \"redrun lint:bin\"",
    "watch:lint:client": "redrun watcher:js -- \"redrun lint:client\"",
    "watch:lint:server": "redrun watcher:js -- \"redrun lint:server\"",
    "watch": "redrun -p watch:*",
    "mkdir": "mkdirp dist dist-dev",
    "clean": "rimraf dist dist-dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudcmd/deepword"
  },
  "keywords": [
    "editor",
    "express",
    "middleware",
    "monaco",
    "patch",
    "diff",
    "ide",
    "text"
  ],
  "author": "coderaiser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cloudcmd/deepword/issues"
  },
  "homepage": "https://github.com/cloudcmd/deepword",
  "dependencies": {
    "ashify": "^1.0.1",
    "currify": "^2.0.0",
    "es6-promisify": "^6.0.0",
    "express": "^4.14.0",
    "fullstore": "^1.0.0",
    "load.js": "^1.1.4",
    "monaco-editor": "^0.13.0",
    "patchfile": "^1.0.1",
    "readjson": "^1.1.0",
    "restafary": "^3.1.0",
    "socket-file": "^2.0.0",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "async": "^2.6.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "clean-css-loader": "^1.0.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "cssnano": "^4.0.0",
    "daffy": "^1.0.3",
    "emitify": "^3.0.2",
    "es6-promise": "^4.0.5",
    "eslint": "^5.0.0",
    "eslint-plugin-node": "^6.0.0",
    "file-loader": "^1.1.6",
    "jssha": "^2.2.0",
    "load.js": "^1.1.4",
    "minifyify": "^7.3.3",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.10.2",
    "nyc": "^12.0.2",
    "redrun": "^6.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^6.0.0",
    "sinon-called-with-diff": "^2.0.0",
    "smalltalk": "^3.0.0",
    "style-loader": "^0.21.0",
    "tape": "^4.6.2",
    "try-catch": "^2.0.0",
    "url-loader": "^1.0.1",
    "watchify": "^3.7.0",
    "webpack": "^4.0.1",
    "webpack-cli": "^3.0.1",
    "zipio": "^1.0.2"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
