{
  "plugins":[
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ],
  "env": {
    "server": {
      "presets": [
        "es2015"
      ]
    },
    "rollup": {
      "presets": [
        "es2015-rollup",
        "es2017"
      ]
    },
    "browserify": {
      "plugins": [
        "add-module-exports"
      ],
      "presets": [
        "es2015",
        "es2017"
      ]
    }
  }
}
